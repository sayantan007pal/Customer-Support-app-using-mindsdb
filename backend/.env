# Environment Configuration
NODE_ENV=development
PORT=3001

# Frontend Configuration
FRONTEND_URL=http://localhost:5174

# MindsDB Configuration (set MOCK_MODE=true for development without MindsDB)
MOCK_MODE=true
MINDSDB_HOST=localhost
MINDSDB_PORT=47334
MINDSDB_USER=mindsdb
MINDSDB_PASSWORD=
MINDSDB_DATABASE=mindsdb

# MindsDB Cloud Configuration (optional)
MINDSDB_USE_CLOUD=false
MINDSDB_CLOUD_EMAIL=your_email@example.com
MINDSDB_CLOUD_PASSWORD=your_password

# MindsDB HTTP URL for SDK connection
MINDSDB_HTTP_URL=http://localhost:47334

# OpenAI Configuration (add your key when available)
OPENAI_API_KEY=your_openai_api_key_here
